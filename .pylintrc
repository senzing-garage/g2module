[pylint]
disable=
    bad-indentation,
    import-error,
    invalid-name,
    line-too-long,
    missing-function-docstring,
    missing-module-docstring,
    no-else-break,
    no-else-raise,
    no-else-return,
    no-member,
    pointless-string-statement,
    raise-missing-from,
    singleton-comparison,
    superfluous-parens,
    syntax-error,
    too-many-arguments,
    too-many-lines,
    too-many-locals,
    too-many-public-methods,
    trailing-newlines,
    trailing-whitespace,
    unidiomatic-typecheck,
    unused-argument,
    unused-import,
    unused-variable,
    unused-wildcard-import,
    useless-object-inheritance,
    wildcard-import,
    wrong-import-position,
good-names=
    template-python
ignore=
    __init__.py,
notes=
    FIXME,
